<view class="cu-timeline" wx:for="{{finallList}}" wx:for-index="key"  wx:for-item="value">
  <view class="cu-time">{{key}}</view>
  <view class="cu-item text-cyan " wx:for="{{value}}">
    <view class="bg-grey content text-cut" bindtap="toarticle" data-obj="{{item}}">
      <text>{{item.time}}</text> {{item.title}}
    </view>
  </view>
</view>
